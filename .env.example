COZE_API_TOKEN=
COZE_BOT_ID=
GOOGLE_API_KEY=
GEMINI_API_KEY=
VERCEL_OIDC_TOKEN=

# ------------------------------------
# Auth.js 必需的密钥
# ------------------------------------
# 你需要生成一个安全的随机字符串。
# 在你的终端运行 `openssl rand -base64 32` 
# 或者使用这个在线工具：https://generate-secret.vercel.app/32
# 把生成的值粘贴在这里：
AUTH_SECRET="粘贴你生成的随机字符串"

# Vercel 部署时会自动处理，但本地开发需要
AUTH_URL="http://localhost:2011"

GOOGLE_CLIENT_ID="Client ID"
GOOGLE_CLIENT_SECRET="Client Secret"

# ------------------------------------
# 代理配置（可选）
# ------------------------------------
# 如果需要使用代理访问外部服务，请配置以下环境变量
# 格式：http://主机地址:端口号
# HTTPS_PROXY="http://127.0.0.1:7890"
# HTTP_PROXY="http://127.0.0.1:7890"

